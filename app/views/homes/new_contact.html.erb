<div class="inner-page">
 <h3>Contact Us</h3>
</div>
<!-- SITE MAIN CONATANT Start -->
<div class="container marginTop30" style="min-height:500px;">

<div class="row-fluid">
  <div class="span7 contact-form">

  <p style="color: #888888;float: left; margin-left: 10px;font-size: 14px;">
    Tell us what you think about our Web site, our services, our performance, how your order was handled or anything else that you feel would be helpful. We welcome all of your comments and suggestions. There are numerous ways to contact Aa EXPRESS DELIVERY INC.:
  </p>

    <%= form_for(@contact,url: save_contact_path) do |f| %>
     <div class="control-group">
      <label class="control-label" for="inputEmail">Your Name<span style="color:red"> *</span></label>
      <%= text_field_tag "contact[name]", nil, :class=>"span12", :required =>'required'%>
     </div>

     <div class="control-group">
      <label class="control-label" for="inputEmail">Your Email<span style="color:red"> *</span></label>
      <%= email_field_tag "contact[email]", nil, :class=>"span12", :required =>'required'%>
     </div>

     <div class="control-group">
      <label class="control-label" for="inputEmail">Subject</label>
      <%= f.text_field :subject, :class=>"span12" %>
     </div>

     <div class="control-group">
      <label class="control-label" for="inputEmail">Query</label>
      <%= f.text_area :query, :class=>"span12", :rows=>3 %>
     </div>
     <div class="control-group">
      <%= f.submit 'Submit', :class=>"btn btn-success marginTop20 marginBottom10"%>
     </div>
    <% end %>
  </div><!-- Span Closed -->
  <div class="span5 ContactAdd">
     <h3><strong>Headquarters</strong></h3>
     <i class="fa fa-map-marker marginTop10"></i> <p>Aa EXPRESS DELIVERY INC..<br />417 W. Arden ave Unit 114c<br />Glendale CA 91203</p>
      <div class="clearfix"></div>
     <i class="fa fa-phone marginTop10"></i><p>Tell: 905-040-3062 <br />Fax: 946-666-4707</p>
     <div class="clearfix"></div>
     <i class="fa fa-envelope marginTop10"></i><p>Email:<br />Web: www.aaexpressdelivery.com</p>
     <div class="clearfix"></div>
     <i class="fa fa-user marginTop10"></i><p><strong>Department Contacts</strong><br/><strong>Customer Service:</strong> Derou Biglari, Manager<br/><strong>Sales Information:</strong> Verjinia Yesaian<br/><strong>Operations:</strong> Andranik Gyumushyan</p>
     <div class="clearfix"></div>
    </div>

</div>

</div>

<!-- SITE MAIN CONATANT Close -->
